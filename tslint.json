{
	"rules": {
		"no-string-throw": true,
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"curly": true,
		"class-name": true,
		"semicolon": [
			true,
			"always"
		],
        "triple-equals": true,
        "typedef": [
            true,
            "call-signature",
            "arrow-call-signature",
            "property-declaration",
            "member-variable-declaration",
            "object-destructuring",
            "array-destructuring"
        ]
	},
	"defaultSeverity": "warning"
}
